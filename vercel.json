{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "server/index.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|map))",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/index.ts"
    }
  ]
}
